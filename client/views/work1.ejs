          $('form').submit(function (element) {
            var myForm = $('form')
            var data = {}
            element.preventDefault();
            for (let value of $(this).serializeArray()) {
              data[value.name] = value.value
            }
            $.post(url(), data,
               (response, status) => {
                var content;
                if (response.status) {
                  $('.error').text(response.message)
                  $('.error').css('color', 'white')
                } else {
                  $('.error').text(response.message)
                  $('.error').css('color', '#a52929')
                }
                $('form').append(content)
                $('form').trigger('reset')
              })
          })

        })
